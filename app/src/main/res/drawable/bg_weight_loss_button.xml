           <size android:height="20dp" android:width="0dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item android:left="8dp" android:top="8dp" android:bottom="8dp" android:right="8dp">
        <shape android:shape="oval">
            <stroke android:color="@color/white" android:width="4dp" />

        </shape>
    </item>
    <item >
        <shape android:shape="oval">
            <stroke android:color="@color/white" android:width="4dp" />

        </shape>
    </item>

    <item android:left="4dp" android:top="4dp" android:bottom="4dp" android:right="4dp">
        <shape android:shape="oval">
            <stroke android:color="@color/Orange" android:width="5dp" />
        </shape>
    </item>





</layer-list>